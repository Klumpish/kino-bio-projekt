<body>
<%- include("./partials/header.ejs") %>
  <div class="movie__single__container">
    <h1 class="movie__single__header"><%=movie.attributes.title%></h1>
    <article class="movie__single">
      <div class="averageRating__container">
        <label><strong>Av. Rating</strong></label>
      </div>
      <%if (movie.attributes.image) {%>
      <img class="movie__single__img" src="<%=movie.attributes.image.url%>" />
      <%}%>
      <div class="movie__single__intro"><%-movie.attributes.intro%></div>
      <a class="primary_button__back" href="/movies">Back to movies page</a>
    </article>
    <div class="screening__info-container">
      <h3 class="screening__info-container-header">Filmen går följande tider</h3>
        <ul class="screening__info-list"></ul>
      </div>
  <div class="reviews__container">
    <h1>Reviews</h1>
    <article>
      <section class="review"></section>
    </article>
    </div>

  </div>
<%- include("./partials/footer.ejs") %>
</body>
